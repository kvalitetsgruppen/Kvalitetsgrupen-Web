@{
  ViewData["Title"] = "Leverantörer";
  Layout = "_ContentNavbarLayout";
}

<div class="container-xxl flex-grow-1 container-p-y">
  <h4 class="fw-bold py-3 mb-4">Leverantörer</h4>

  <!-- Stats Cards -->
  <div class="row mb-4">
    <!-- Suppliers Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Leverantörer</h5>
            <h3 class="mb-1">143</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-primary d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-store bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- Not Qualified Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Inte kvalificerad</h5>
            <h3 class="mb-1">40</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-danger d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-x-circle bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- Qualified Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Kvalificerad</h5>
            <h3 class="mb-1">46</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-success d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-check-circle bx-lg"></i>
          </div>
        </div>
      </div>
    </div>

    <!-- No Record Card -->
    <div class="col-md-3">
      <div class="card">
        <div class="card-body d-flex justify-content-between align-items-center">
          <div class="text-left">
            <h5 class="card-title mb-1">Ingen post</h5>
            <h3 class="mb-1">57</h3>
            <small>Totalt antal</small>
          </div>
          <div class="avatar-initial rounded bg-label-secondary d-flex align-items-center justify-content-center"
            style="width: 50px; height: 50px;">
            <i class="bx bx-file-find bx-lg"></i>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Filters -->
  <div class="card mb-4">
    <div class="card-body">
      <form class="row g-3">
        <div class="col-md-4">
          <select class="form-select">
            <option selected>Välj klassificering</option>
            <option>Nivå 1 - Huvudnivå </option>
            <option>Nivå 2 - Sekundär</option>
            <option>Nivå 3 - Administrativ</option>
            <option>Nivå 4 - Övrigt</option>
            <option>Inte aktiv</option>
            <option>Utsikt</option>
          </select>
        </div>
        <div class="col-md-4">
          <select class="form-select">
            <option selected>Välj utvärderingsnivå</option>
            <option>Kvalificerad</option>
            <option>Inte kvalificerad</option>
            <option>Ingen post</option>
          </select>
        </div>
        <div class="col-md-4">
          <select class="form-select">
            <option selected>Välj utvärderingsdatum</option>
            <option>Senaste 3 månaderna</option>
            <option>Senaste 6 månaderna</option>
            <option>I år</option>
            <option>Förra året</option>
            <option>Senaste 3 åren</option>
            <option>Alla år</option>
            <option>Inget utvärderingsdatum</option>
          </select>
        </div>
      </form>
    </div>
  </div>

